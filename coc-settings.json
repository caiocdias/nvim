{
  "suggest.autoTrigger": "always",
  "languageserver": {
    "rust": {
      "command": "rust-analyzer",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"],
      "initializationOptions": {},
      "settings": {
        "rust-analyzer": {
          "cargo": {
            "loadOutDirsFromCheck": true
          },
          "procMacro": {
            "enable": true
          }
        }
      }
    }
}
}
